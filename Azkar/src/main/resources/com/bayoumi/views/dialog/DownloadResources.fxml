   <children>
      <Label fx:id="title" alignment="CENTER" maxWidth="1.7976931348623157E308" style="-fx-background-color: -fx-secondary; -fx-text-fill: white; -fx-effect: dropshadow(three-pass-box, rgba(0, 0, 0, 0.4), 20, 0, 0, 0);" text="تحميل موارد البرنامج">
